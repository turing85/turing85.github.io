<header class="header" role="banner">
  <nav class="navbar">
    <div class="navbar-brand">
      <div class="navbar-item">
        <img class="navbar-image hide-for-print pill penpen" src="{{{siteRootPath}}}/static/images/PenPenRound.png" alt="Avatar"/>
      </div>
      <div class="navbar-item">
        <a class ="navbar-item hide-for-print" href="{{siteRootPath}}/about/me">Marco Bungart</a>
      </div>
      <button class="navbar-burger" data-target="topbar-nav">
        <span></span>
        <span></span>
        <span></span>
      </button>
    </div>
    <div id="topbar-nav" class="navbar-menu">
      <div class="navbar-end">
{{#each (component-name-not 'about')}}
        <div class="navbar-end navbar-item hide-for-print">
          <a class="navbar-end navbar-item pill hide-for-print is-hoverable hoverable-black" id="{{{./name}}}" href="{{{relativize ./url}}}">{{{./title}}}</a>
        </div>
{{/each}}
{{#if env.SITE_SEARCH_PROVIDER}}
        <div class="navbar-item search hide-for-print">
          <div id="navbar-item search-field" class="field has-filter">
